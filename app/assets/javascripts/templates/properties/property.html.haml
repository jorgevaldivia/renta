                %h5
                  %strong ${{ record.object.total_revenue | number:2 }}
                  = Property.human_attribute_name(:revenue)
              .col-md-4
                %span.line{:style => "display: none;"} 5,3,9,6,5,9,7,3,5,2
                %svg.peity{:height => "16", :width => "32"}
                  %polygon{:fill => "#1ab394", :points => "0 15 0 7.166666666666666 3.5555555555555554 10.5 7.111111111111111 0.5 10.666666666666666 5.5 14.222222222222221 7.166666666666666 17.77777777777778 0.5 21.333333333333332 3.833333333333332 24.888888888888886 10.5 28.444444444444443 7.166666666666666 32 12.166666666666666 32 15"}
