                %h5
                  %strong ${{ record.object.total_expenses | number:2 }}
                  = Property.human_attribute_name(:expenses)
              .col-md-4
                %span.bar{:style => "display: none;"} 5,3,2,-1,-3,-2,2,3,5,2
                %svg.peity{:height => "16", :width => "32"}
                  %rect{:fill => "#1ab394", :height => "10", :width => "2.3", :x => "0", :y => "0"}
                  %rect{:fill => "#d7d7d7", :height => "6", :width => "2.3", :x => "3.3", :y => "4"}
