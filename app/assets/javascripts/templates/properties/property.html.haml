                  %rect{:fill => "#d7d7d7", :height => "2", :width => "2.3", :x => "9.899999999999999", :y => "10"}
                  %rect{:fill => "#1ab394", :height => "6", :width => "2.3", :x => "13.2", :y => "10"}
                  %rect{:fill => "#d7d7d7", :height => "4", :width => "2.3", :x => "16.5", :y => "10"}
                  %rect{:fill => "#1ab394", :height => "4", :width => "2.3", :x => "19.799999999999997", :y => "6"}
                  %rect{:fill => "#d7d7d7", :height => "6", :width => "2.3", :x => "23.099999999999998", :y => "4"}
                  %rect{:fill => "#1ab394", :height => "10", :width => "2.3", :x => "26.4", :y => "0"}
                  %rect{:fill => "#d7d7d7", :height => "4", :width => "2.3", :x => "29.7", :y => "6"}
                %h5
                  %strong ${{ record.object.total_profit | number:2 }}
                  = Property.human_attribute_name(:profit)

