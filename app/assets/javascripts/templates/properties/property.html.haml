              .col-md-4
                %span.bar{:style => "display: none;"} 5,3,9,6,5,9,7,3,5,2
                %svg.peity{:height => "16", :width => "32"}
                  %rect{:fill => "#1ab394", :height => "8.88888888888889", :width => "2.3", :x => "0", :y => "7.111111111111111"}
