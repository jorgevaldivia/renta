.container-fluid.pt-md
  .row
    .col-md-4
      .panel.ibox.float-e-margins
        .panel-heading
          .panel-title.ibox-title
            %h5 {{ record.object.name }}
        %div
          .ibox-content.no-padding.border-left-right
            %img.img-responsive{alt: "image", src: "assets/demo/avatars/8.jpg"}/
          .panel-body.ibox-content.profile-content

            .user-button
              .row
                .col-md-12
                  .btn-group.btn-block{dropdown: ""}
                    %button.btn.btn-success.btn-block.dropdown-toggle{type: "button", "data-toggle" => "dropdown"}
                      %span.fa.fa-cogs
                      = I18n.t(:settings)
                      %span.caret
                    %ul.dropdown-menu.pull-right{role: "menu"}
                      %li
                        %a{"ui-sref" => "default.properties.property.edit({id: record.object.id})"}
                          %span.fa.fa-pencil.fa-fw
                          = I18n.t(:edit_object, name: Property.model_name.human)
                        %a{href: "", "ng-really-message" => I18n.t("confirmations.deletion"), "ng-really-click" => "delete()"}
                          %span.fa.fa-trash-o.fa-fw
                          = I18n.t(:delete)
                        %a{"data-target" => "#transaction-modal", "data-toggle" => "modal", style: "cursor: pointer"}
                          %span.fa.fa-money.fa-fw
                          = I18n.t(:add, name: Transaction.model_name.human)
                / .col-md-6
                /   %a.btn.btn-success.btn-block{"ui-sref" => "default.properties.property.edit({id: record.object.id})"}
                /     %span.fa.fa-pencil
                /     = I18n.t(:edit_object, name: Property.model_name.human)
                / .col-md-6
                /   %a.btn.btn-danger.btn-block{href: "", "ng-really-message" => I18n.t("confirmations.deletion"), "ng-really-click" => "delete()"}
                /     %span.fa.fa-trash-o
                /     = I18n.t(:delete)


            / %hr.mnr-md.mnl-md/
            %br/

            / %h4
            /   %strong Monica Smith
            %p
              %i.fa.fa-map-marker.fa-fw
              %small {{ record.object.full_address }}
            %p
              %i.fa.fa-bed.fa-fw
              %small
                {{ record.object.beds }}
                = Property.human_attribute_name(:beds)
            %p
              %i.fa.fa-male.fa-fw
              %small
                {{ record.object.baths }}
                = Property.human_attribute_name(:baths)
            %p
              %i.fa.fa-building.fa-fw
              %small
                {{ record.object.size }}
                m
                %sup 2
            / %h5
            /   About me
            / %p
